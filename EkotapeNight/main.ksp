				set_engine_par($ENGINE_PAR_TUNE, 500000 + ($tune * 13889), %normal_groups[$i], -1, -1)
		end if
		
		inc($i)
		set_text ($tune_label,get_engine_par_disp($ENGINE_PAR_TUNE,5,-1,-1))
		// message(get_engine_par_disp($ENGINE_PAR_TUNE, %normal_groups[$i], -1, -1) & " " & $tune)
	end while

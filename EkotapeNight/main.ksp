on ui_control($drift)
	$i := 0
	while($i <= $num_reverse_groups)
		if (get_control_par(get_ui_id($drift),$CONTROL_PAR_KEY_ALT) = 1)
			$drift := $drift_defval 
			else
				set_engine_par($ENGINE_PAR_INTMOD_INTENSITY,$drift,%reverse_groups[$i],find_mod(%reverse_groups[$i],"SAMPLE_START"),-1)
		end if
		// message(get_engine_par($ENGINE_PAR_INTMOD_INTENSITY, 0, find_mod(%reverse_groups[$i],"SAMPLE_START"), -1))
		inc($i)
	end while
end on

on ui_control($reverse)
	if($reverse_bypass = 0)
		$i := 0
		if (get_control_par(get_ui_id($reverse),$CONTROL_PAR_KEY_ALT) = 1)
			while($i < $num_reverse_groups)
				set_engine_par($ENGINE_PAR_VOLUME, 0, %reverse_groups[$i],-1 , -1)
				set_engine_par($ENGINE_PAR_VOLUME, %vol_ep_rev[1000], %normal_groups[$i],-1 , -1)
				$reverse := 0
				inc($i)
			end while
			else
				if($reverse < 1001)
					$i := 0
					while($i < $num_reverse_groups)
						set_engine_par($ENGINE_PAR_VOLUME, %vol_ep_rev[$reverse], %reverse_groups[$i],-1 , -1)
						// message(get_engine_par_disp($ENGINE_PAR_VOLUME, %normal_groups[1], -1 , -1) & " " & get_engine_par_disp($ENGINE_PAR_VOLUME, %reverse_groups[1], -1 , -1) & " " & %normal_groups[1])
						inc($i)
					end while
					else
						i := 0
						while($i < $num_reverse_groups)
							set_engine_par($ENGINE_PAR_VOLUME, %vol_ep_rev[2000 - $reverse], %normal_groups[$i],-1 , -1)
							inc($i)
							// message(get_engine_par_disp($ENGINE_PAR_VOLUME, %normal_groups[1], -1 , -1) & " " & get_engine_par_disp($ENGINE_PAR_VOLUME, %reverse_groups[1], -1 , -1) & " " & %reverse_groups[1] & " " & %vol_ep_rev[1000 - $reverse] & " " & $reverse)
						end while
				end if
		end if
	end if
end on

on ui_control($reverse_bypass)
	if($reverse_bypass = 1)
		$rev := $reverse
		$i := 0
		while($i < $num_reverse_groups)
						set_engine_par($ENGINE_PAR_VOLUME, %vol_ep_rev[1000], %normal_groups[$i],-1 , -1)
						set_engine_par($ENGINE_PAR_VOLUME, 0, %reverse_groups[$i],-1 , -1)
						inc($i)
		end while
		else
			while($i < $num_reverse_groups)
						set_engine_par($ENGINE_PAR_VOLUME, %vol_ep_rev[2000 - $reverse], %normal_groups[$i],-1 , -1)
						set_engine_par($ENGINE_PAR_VOLUME, $rev, %vol_ep_rev[$reverse],-1 , -1)
						inc($i)
			end while
	end if
end on


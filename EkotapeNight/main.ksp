on ui_control($verb_bypass)
	if($verb_bypass = 1)
		set_engine_par($ENGINE_PAR_SEND_EFFECT_OUTPUT_GAIN, %vol_ep[0], -1, 6, $NI_MAIN_BUS)
		set_engine_par($ENGINE_PAR_SEND_EFFECT_DRY_LEVEL, %vol_ep[334], -1, 6, $NI_MAIN_BUS)
		else
			if($echo_mix < 335)
				set_engine_par($ENGINE_PAR_SEND_EFFECT_OUTPUT_GAIN, %vol_ep[$echo_mix], -1, 6, $NI_MAIN_BUS)
				set_engine_par($ENGINE_PAR_SEND_EFFECT_DRY_LEVEL, %vol_ep[334], -1, 6, $NI_MAIN_BUS)
				else
					set_engine_par($ENGINE_PAR_SEND_EFFECT_OUTPUT_GAIN, %vol_ep[334], -1, 6, $NI_MAIN_BUS)
					set_engine_par($ENGINE_PAR_SEND_EFFECT_DRY_LEVEL, %vol_ep[669 - $verb_bypass], -1, 6, $NI_MAIN_BUS)
			end if
			set_text ($echo_mix_label, ceil(int_to_real($verb_bypass)/670.0 * 100.0))
	end if

macro OnOff(#start#, #end#)

	if((#start# > 1 or #start# = -1) and (#start# # 1 and #end# # 1))
		$range_btn_1 := 0
	else
		$range_btn_1 := 1
	end if

	if((#start# > 2 or #start# = -1) and (#start# # 2 and #end# # 2))
		$range_btn_2 := 0
	else
		if(#start# <= 2 and #end# >= 2)
			$range_btn_2 := 1
		end if
	end if

	if((#start# > 3 or #start# = -1) and (#start# # 3 and #end# # 3))
		$range_btn_3 := 0
	else
		if(#start# <= 3 and #end# >= 3)
			$range_btn_3 := 1
		end if
	end if

	if((#start# > 4 or #start# = -1) and (#start# # 4 and #end# # 4))
		$range_btn_4 := 0
	else
		if(#start# <= 4 and #end# >= 4)
			$range_btn_4 := 1
		end if
	end if

	if((#start# > 5 or #start# = -1) and (#start# # 5 and #end# # 5))
		$range_btn_5 := 0
	else
		if(#start# <= 5 and #end# >= 5)
			$range_btn_5 := 1
		end if
	end if

	if((#start# > 6 or #start# = -1) and (#start# # 6 and #end# # 6))
		$range_btn_6 := 0
	else
		if(#start# <= 6 and #end# >= 6)
			$range_btn_6 := 1
		end if
	end if


	if((#end# < 2 or #start# = -1) and (#start# # 2 and #end# # 2))
		$range_btn_2 := 0
	else 
		if(#end# >= 2 and #start# <= 2)
			$range_btn_2 := 1
		end if
	end if

	if((#end# < 3 or #start# = -1) and (#start# # 3 and #end# # 3))
		$range_btn_3 := 0
	else 
		if(#end# >= 3 and #start# <= 3)
			$range_btn_3 := 1
		end if
	end if

	if((#end# < 4 or #start# = -1) and (#start# # 4 and #end# # 4))
		$range_btn_4 := 0
	else 
		if(#end# >= 4 and #start# <= 4)
			$range_btn_4 := 1
		end if
	end if

	if((#end# < 5 or #start# = -1) and (#start# # 5 and #end# # 5))
		$range_btn_5 := 0
	else 
		if(#end# >= 5 and #start# <= 5)
			$range_btn_5 := 1
		end if
	end if

	if((#end# < 6 or #start# = -1) and (#start# # 6 and #end# # 6))
		$range_btn_6 := 0
	else 
		if(#end# >= 6 and #start# <= 6)
			$range_btn_6 := 1
		end if
	end if
